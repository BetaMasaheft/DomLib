"&gt;&amp;#4776;&amp;#4632;&amp;#4961;&lt;/span&gt;&lt;span lang="EN-GB" unicode="" arial="" style="line-height: 115%;"&gt; &lt;/span&gt;&lt;span lang="EN-GB" style="line-height: 115%; font-family: Nyala;"&gt;&amp;#4725;&amp;#4661;&amp;#4648;&amp;#4841;&amp;#4961;&lt;/span&gt;&lt;span lang="EN-GB" unicode="" arial="" style="line-height: 115%;"&gt; &lt;/span&gt;&lt;span lang="EN-GB" style="line-height: 115%; font-family: Nyala;"&gt;&amp;#4618;&amp;#4720;&amp;#4961;&lt;/span&gt;&lt;span lang="EN-GB" unicode="" arial="" style="line-height: 115%;"&gt; &lt;/span&gt;&lt;span lang="EN-GB" style="line-height: 115%; font-family: Nyala;"&gt;&amp;#4789;&amp;#4622;&amp;#4961;&lt;/span&gt;&lt;span lang="EN-GB" unicode="" arial="" style="line-height: 115%;"&gt; &lt;/span&gt;&lt;span lang="EN-GB" style="line-height: 115%; font-family: Nyala;"&gt;&amp;#4768;&amp;#4704;&amp;#4659;&amp;#4840;&amp;#4961; &amp;#4824;&amp;#4768;&amp;#4704;&amp;#4661;&amp;#4777;&amp;#4961; &amp;#4616;&amp;#4781;&amp;#4633;&amp;#4961; &amp;#4704;&amp;#4773;&amp;#4757;&amp;#4720;&amp;#4961; &amp;#4629;&amp;#4928;&amp;#4720;&amp;#4961; &amp;#4768;&amp;#4773;&amp;#4637;&amp;#4654;&amp;#4725;&amp;#4840;&amp;#4961; &amp;#4808;&amp;#4776;&amp;#4632;&amp;#4961; &amp;#4770;&amp;#4725;&amp;#4653;&amp;#4661;&amp;#4817;&amp;#4754;&amp;#4961; &amp;#4704;&amp;#4874;&amp;#4828;&amp;#4961; &amp;#4920;&amp;#4622;&amp;#4725;&amp;#4781;&amp;#4633;&amp;#4961; &amp;#4773;&amp;#4632;&amp;#4610;&amp;#4961; &amp;#4704;&amp;#4629;&amp;#4845;&amp;#4808;&amp;#4725;&amp;#4840;&amp;#4961; &amp;#4808;&amp;#4773;&amp;#4632;&amp;#4610;&amp;#4961; &amp;#4704;&amp;#4638;&amp;#4725;&amp;#4840;&amp;#4961; &amp;#4618;&amp;#4720;&amp;#4961; &amp;#4616;&amp;#4739;&amp;#4901;&amp;#4773;&amp;#4961; &amp;#4920;&amp;#4624;&lt;/span&gt;&lt;/span&gt;&lt;span style="font-family: Arial;"&gt;&lt;span style="font-size: small;"&gt;&lt;span lang="EN-GB" style="line-height: 115%;"&gt;&lt;span lang="EN-GB" unicode="" arial="" style="line-height: 115%;"&gt;