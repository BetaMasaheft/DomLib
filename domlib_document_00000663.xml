<additionals class="DLAdditional" heritable="false" notinherit="false"><additional xml:lang="en" inherited="0"><cont>&lt;p&gt;&lt;span style="font-family: Arial;"&gt;&lt;span style="font-size: small;"&gt;&lt;span lang="EN-GB" unicode="" new="" style="line-height: 115%;"&gt;Fol. 115va: Ownership note.&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;</cont><add>&lt;p style="text-align: left;"&gt;&lt;span style="font-family: Arial;"&gt;&lt;span style="font-size: small;"&gt;&lt;span lang="EN-GB" unicode="" new="" arial="" style="line-height: 115%;"&gt;Fol. 115va:&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;span lang="EN-GB" unicode="" new="" arial="" style="line-height: 115%; font-family: Nyala; font-size: 14pt;"&gt; &lt;/span&gt;&lt;span lang="AMH" unicode="" new="" arial="" style="line-height: 115%; font-family: Nyala; font-size: 14pt;"&gt;&amp;#4829;&amp;#4677;&amp;#4851;&amp;#4660;&amp;#4961; &amp;#4824;&amp;#4848;&amp;#4709;&amp;#4648;&amp;#4961; &amp;#4846;&amp;#4624;&amp;#4757;&amp;#4661;&amp;#4961; &amp;#4632;&amp;#4901;&amp;#4637;&amp;#4677;&amp;#4961; &amp;#4773;&amp;#4757;&amp;#4720;&amp;#4961; &amp;#4845;&amp;#4773;&amp;#4722;&amp;#4961; &amp;#4848;&amp;#4709;&amp;#4648;&amp;#4961; &amp;#4708;&amp;#4724;&amp;#4621;&amp;#4961; &amp;#4784;&amp;#4784;&amp;#4622;&amp;#4961;&lt;/span&gt;&lt;span lang="EN-GB" unicode="" new="" arial="" style="line-height: 115%; font-family: Nyala; font-size: 14pt;"&gt;&amp;lt;...&amp;gt;&lt;/span&gt;&lt;span lang="AMH" unicode="" new="" arial="" style="line-height: 115%; font-family: Nyala; font-size: 14pt;"&gt;&amp;#4846;&amp;#4624;&amp;#4757;&amp;#4661;&amp;#4961; &amp;#4632;&amp;#4901;&amp;#4637;&amp;#4677;&amp;#4961; &amp;#4824;&amp;#4656;&amp;#4648;&amp;#4678;&amp;#4961; &amp;#4808;&amp;#4824;&amp;#4936;&amp;#4624;&amp;#4678;&amp;#4961; &amp;#4813;&amp;#4873;&amp;#4824;&amp;#4961; &amp;#4616;&amp;#4845;&amp;#4777;&amp;#4757;&amp;#4961; &amp;#4920;&amp;#4624;&amp;#4938;&amp;#4609;&amp;#4961; &amp;#4872;&amp;#4709;&amp;#4648;&amp