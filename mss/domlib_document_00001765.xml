<additionals class="DLAdditional" heritable="false" notinherit="false"><additional xml:lang="en" inherited="0"><cont>&lt;p&gt;&lt;span style="font-size: small"&gt;&lt;span style="line-height: 115%"&gt;Fol&lt;/span&gt;&lt;span style="line-height: 115%"&gt;. 65va: Donation note.&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;</cont><add>&lt;p&gt;&lt;span style="font-size: small"&gt;&lt;span style="line-height: 115%"&gt;Fol. 65va: &lt;/span&gt;&lt;/span&gt;&lt;span style="font-size: large"&gt;&lt;span style="line-height: 115%"&gt;&amp;#4829;&amp;#4720;&amp;#4768;&amp;#4637;&amp;#4648;&amp;#4961; &amp;#4635;&amp;#4653;&amp;#4843;&amp;#4637;&amp;#4961; &amp;#4824;&amp;#4768;&amp;#4875;&amp;#4637;&amp;#4846;&amp;#4961; &amp;#4677;&amp;#4849;&amp;#4661;&amp;#4961; &amp;#4634;&amp;#4779;&amp;#4772;&amp;#4621;&amp;#4961; &amp;#4824;&amp;#4808;&amp;#4608;&amp;#4710;&amp;#4961; &amp;#4616;&amp;#4677;&amp;#4849;&amp;#4661;&amp;#4961; &amp;#4634;&amp;#4779;&amp;#4772;&amp;#4621;&amp;#4961; &amp;#4776;&amp;#4632;&amp;#4961; &amp;#4845;&amp;#4789;&amp;#4758;&amp;#4633;&amp;#4961; &amp;#4616;&amp;#4632;&amp;#4853;&amp;#4736;&amp;#4754;&amp;#4720;&amp;#4961; &amp;#4645;&amp;#4875;&amp;#4961; &amp;#4808;&amp;#4752;&amp;#4941;&amp;#4661;&amp;#4962; &amp;#4616;&amp;#4819;&amp;#4616;&amp;#4632;&amp;#4961; &amp;#4819;&amp;#4616;&amp;#4637;&amp;#4961; &amp;#4808;&amp;#4920;&amp;#4624;&amp;#4938;&amp;#4611;&amp;#4962; &amp;#4808;&amp;#4621;&amp;#4848;&amp;#4961; &amp;#4633;&amp;#4660;&amp;#4962; &amp;#4808;&amp;#4824;&amp;#4768;&amp;#4925;&amp;#4624;&amp;#4939;&amp;#4961; &amp;#4808;&amp;#4621;&amp;#4848;&amp;#4961; &amp;#4634;&amp;#4779;&amp;#4772;&amp;#4621;&amp;#4961; &amp;#4808;&amp;#4709;&amp;#4773;&amp;#4658;&amp;#4721; &amp;#4808;&amp;#4616;&amp;#4720;&amp;#4961; &amp;#4811;&amp;#4613;&amp;#4853;&amp;#4961; &amp;#4808;&amp;#4848;&amp;#4674;&amp;#4678;&amp;#4633;&amp;#4961; &amp;#4720;&amp;#4645;&amp;#4939;&amp;#4961; &amp;#4634;&amp;#4779;&amp;#4772;&amp;#4621;&amp;#4961; &amp;#4824;&amp;#4634;