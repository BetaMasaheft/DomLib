6;&amp;#4629;&amp;#4845;&amp;#4808;&amp;#4720;&amp;#4961; &amp;#4645;&amp;#4875;&amp;#4961; &amp;#4808;&amp;#4752;&amp;#4941;&amp;#4661;&amp;#4961; &amp;#4808;&amp;#4843;&amp;#4757;&amp;#4709;&amp;#4654;&amp;#4633;&amp;#4961; &amp;#4813;&amp;#4661;&amp;#4720;&amp;#4961; &amp;#4656;&amp;#4635;&amp;#4843;&amp;#4810;&amp;#4725;&amp;#4961; &amp;#4624;&amp;#4851;&amp;#4661;&amp;#4961; &amp;#4616;&amp;#4819;&amp;#4616;&amp;#4632;&amp;#4961; &amp;#4819;&amp;#4616;&amp;#4637;&amp;#4962; &amp;#4824;&amp;#4640;&amp;#4648;&amp;#4678;&amp;#4961; &amp;#4808;&amp;#4824;&amp;#4936;&amp;#4624;&amp;#4678;&amp;#4961; &amp;#4704;&amp;#4645;&amp;#4621;&amp;#4899;&amp;#4752;&amp;#4961; &amp;#4916;&amp;#4901;&amp;#4654;&amp;#4661;&amp;#4961; &amp;#4808;&amp;#4915;&amp;#4813;&amp;#4622;&amp;#4661;&amp;#4961; &amp;#4813;&amp;#4873;&amp;#4824;&amp;#4961; &amp;#4616;&amp;#4845;&amp;#4777;&amp;#4757;&amp;#4962; &lt;/span&gt;&lt;span style="line-height: 115%;"&gt;&amp;#4808;&amp;#4920;&amp;#4624;&amp;#4938;&amp;#4609;&amp;#4961; &amp;#4739;&amp;#4901;&amp;#4773;&amp;#4961; &amp;#4808;&amp;#4768;&amp;#4707;&amp;#4658;&amp;#4961; &amp;#4819;&amp;#4661;&amp;#4704;&amp;#4961; &amp;#4853;&amp;#4757;&amp;#4877;&amp;#4621;&amp;#4961; &amp;#4704;&amp;#4925;&amp;#4619;&amp;#4622;&amp;#4725;&amp;#4776;&amp;#4961; &amp;#4781;&amp;#4853;&amp;#4758;&amp;#4961; &amp;#4773;&amp;#4638;&amp;#4720;&amp;#4961; &amp;#4752;&amp;#4941;&amp;#4661;&amp;#4961; &amp;#4925;&amp;#4753;&amp;#4821;&amp;#4961; &amp;#4808;&amp;#4773;&amp;#4638;&amp;#4720;&amp;#4961; &amp;#4645;&amp;#4875;&amp;#4961; &amp;#4824;&amp;#4624;&amp;#4882;&amp;#4621;&amp;#4961; &amp;#4774;&amp;#4768;&amp;#4635;&amp;#4753;&amp;#4772;&amp;#4621;&amp;#4962;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;</add><det>Written in the same hand as the main text.&#13;
